<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.manager.model.MUserInfo" table="M_USER_INFo">
		<id name="loginName" >
			<generator class="assigned"/>
		</id>
		
		<property name="password" type="java.lang.String" column="PASSWORD" />
		<property name="role" type="java.lang.Integer" column="ROLE" />
		<property name="status" type="java.lang.Integer" column="STATUS" />
		<property name="name" type="java.lang.String" column="NAME" />
		<property name="employeeId" type="java.lang.String" column="EMPLOYEEID" />
		<property name="position" type="java.lang.String" column="POSITION" />
		<property name="tel" type="java.lang.String" column="TEL" />
		<property name="birthday" type="java.util.Date" column="BIRTHDAY" />
		<property name="photo" type="java.lang.String" column="PHOTO" />
		<property name="registerDate" type="java.util.Date" column="REGISTERDATE" />
		<property name="registerUser" type="java.lang.String" column="REGISTERUSER" />
		<property name="modifyDate" type="java.util.Date" column="MODIFYDATE" />
		<property name="modifyUser" type="java.lang.String" column="MODIFYUSER" />
        <property name="comment" type="java.lang.String" column="COMMENT" />
	</class>
</hibernate-mapping>
