<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.manager.model.MMemberInfo" table="M_MEMBER_INFO">
		<id name="memberId" >
			<generator class="increment"/>
		</id>
		
		<property name="status" type="java.lang.Integer" column="STATUS" />
		<property name="cardId" type="java.lang.String" column="CARDID" />
		<property name="score" type="java.lang.Integer" column="SCORE" />
		<property name="name" type="java.lang.String" column="NAME" />
		<property name="sex" type="java.lang.Integer" column="SEX" />
		<property name="birthday" type="java.util.Date" column="BIRTHDAY" />
		<property name="age" type="java.lang.Integer" column="AGE" />
		<property name="idNo" type="java.lang.String" column="IDNO" />
		<property name="exchangeCode" type="java.lang.String" column="EXCHANGECODE" />
		<property name="requirement" type="java.lang.Integer" column="REQUIREMENT" />
		<property name="mobile" type="java.lang.String" column="MOBILE" />
		<property name="tel" type="java.lang.String" column="TEL" />
		<property name="occupation" type="java.lang.String" column="OCCUPATION" />
		<property name="email" type="java.lang.String" column="EMAIL" />
		<property name="address" type="java.lang.String" column="ADDRESS" />
		<property name="accept" type="java.lang.Integer" column="ACCEPT" />
		<property name="registerDate" type="java.util.Date" column="REGISTERDATE" />
		<property name="registerUser" type="java.lang.String" column="REGISTERUSER" />
		<property name="verifyStatus" type="java.lang.Integer" column="VERIFYSTATUS" />
		<property name="verifier" type="java.lang.String" column="VERIFIER" />
        <property name="comment" type="java.lang.String" column="COMMENT" />
        <property name="isNew" type="java.lang.String" column="ISNEW" />
	</class>
</hibernate-mapping>
